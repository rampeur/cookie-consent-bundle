{
    "name": "rampeur/cookie-consent-bundle",
    "description": "Symfony 4 Bundle for the Cookie Consent Plugin.",
    "keywords": [
        "cookie",
        "consent",
        "insites",
        "policy",
        "rampeur",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/rampeur/cookie-consent-bundle",
    "support": {
        "email": "admin@unespace.com",
        "issues": "https://github.com/rampeur/cookie-consent-bundle/issues",
        "source": "https://github.com/rampeur/cookie-consent-bundle/releases",
        "wiki": "https://github.com/rampeur/cookie-consent-bundle/wiki"
    },
    "authors": [
        {
            "name": "Julien Gautier",
            "homepage": "https://unespace.fr",
            "email": "admin@unespace.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/translation": "~2.8|~3.0|~4.0",
        "symfony/twig-bundle": "~2.8|~3.0|~4.0",

        "twig/twig": "~1.34|~2.0"
    },
    "autoload": {
        "psr-4": {
            "Rampeur\\Bundle\\CookieConsentBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
